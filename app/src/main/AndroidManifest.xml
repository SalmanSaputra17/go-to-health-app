<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.hp.justhealth">

    <uses-permission android:name="android.permission.INTERNET" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="26.1.0"
        tools:replace="android:value" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SigninActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity" />
        <!-- The main/home activity (it has no parent activity) -->
        <activity android:name=".DashboardActivity" />
        <!-- A child of the main activity -->
        <activity android:name=".CheckBBActivity" />
        <activity android:name=".CaloriActivity" />
        <activity android:name=".DayCaloriActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".ProfilActivity" />
        <activity android:name=".FAQsActivity" />
        <activity android:name=".FoodCaloriActivity" />
        <activity android:name=".HeartRateActivity" />
    </application>

</manifest>